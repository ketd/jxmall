<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ketd.product.mapper.BrandMapper">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.ketd.product.domain.Brand" id="BaseResultMap">
            <result property="brandId"    column="brand_id"    />
            <result property="name"    column="name"    />
            <result property="logo"    column="logo"    />
            <result property="descript"    column="descript"    />
            <result property="showStatus"    column="show_status"    />
            <result property="firstLetter"    column="first_letter"    />
            <result property="sort"    column="sort"    />
    </resultMap>

    <sql id="Base_Column_List">
brand_id,name,logo,descript,show_status,first_letter,sort
    </sql>

</mapper>

